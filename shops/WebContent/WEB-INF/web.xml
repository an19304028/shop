<?xml version="1.0" encoding="UTF-8"?>
<web-app>
 <welcome-file-list>
  	<welcome-file>/WEB-INF/userjsp/index.jsp</welcome-file>
 </welcome-file-list>
  <servlet>
    <servlet-name>FrontServlet</servlet-name>
    <servlet-class>FrontServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>FrontServlet</servlet-name>
    <url-pattern>/</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
  	<servlet-name>default</servlet-name>
  	<url-pattern>/image/*</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>default</servlet-name>
  	<url-pattern>/js/lightbox.js</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>header</servlet-name>
    <jsp-file>/WEB-INF/userjsp/header.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>header</servlet-name>
    <url-pattern>/header</url-pattern>
  </servlet-mapping>
  
</web-app>